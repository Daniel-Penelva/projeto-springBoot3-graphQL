type Query{
    listarProdutos : [Produto]
    listarProdutoPorId(id : String) : Produto
    listarCategoria : [Categoria]
    listarCategoriaPorId(id: Float): Categoria
}

type Produto{
     id : String,
    nome : String,
    preco : Float,
    quantidade : Int,
    categoria:Categoria
}

type Categoria{
    id : Float,
    nome : String,
    produtos: [Produto]
}